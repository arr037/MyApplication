<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    
    <FontFamily x:Key="Roboto-Bold">pack://application:,,,/MyApp;Component/Fonts/#Roboto Bold</FontFamily>
    <FontFamily x:Key="Roboto-Italic">pack://application:,,,/MyApp;Component/Fonts/#Roboto Italic</FontFamily>
    <FontFamily x:Key="Roboto-Light">pack://application:,,,/MyApp;Component/Fonts/#Roboto Light</FontFamily>
    <FontFamily x:Key="Roboto-Medium">pack://application:,,,/MyApp;Component/Fonts/#Roboto Medium</FontFamily>
    <FontFamily x:Key="Roboto-Regular">pack://application:,,,/MyApp;Component/Fonts/#Roboto Regular</FontFamily>
    <FontFamily x:Key="Roboto-Thin">pack://application:,,,/MyApp;Component/Fonts/#Roboto Thin</FontFamily>
    
    
    
    <system:Double x:Key="MiniFontSize">14</system:Double>
    <system:Double x:Key="LMiniFontSize">15</system:Double>
    <system:Double x:Key="DefaultFontSize">16</system:Double>
    <system:Double x:Key="SmallFontSize">17</system:Double>
    <system:Double x:Key="LargeFontSize">17</system:Double>
    <system:Double x:Key="VeryLargeFontSize">18</system:Double>
</ResourceDictionary>