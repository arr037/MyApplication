<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Background blue color -->
    <Color x:Key="BackgroundLightBlue">#EAF2FF</Color>
    <SolidColorBrush x:Key="BackgroundLightBlueBrush" Color="{StaticResource BackgroundLightBlue}"/>
    
    <!-- Background blue color -->
    <Color x:Key="BackgroundBlue">#508FF4</Color>
    <SolidColorBrush x:Key="BackgroundBlueBrush" Color="{StaticResource BackgroundBlue}"/>
    
    <!-- Background selected item color -->
    <Color x:Key="BackgroundLightSelectedBlue">#EAF2FF</Color>
    <SolidColorBrush x:Key="BackgroundLightSelectedBlueBrush" Color="{StaticResource BackgroundLightSelectedBlue}"/>
    
    <!-- Foreground  color -->
    <Color x:Key="ForegroundGrayColor">#53585E</Color>
    <SolidColorBrush x:Key="ForegroundGrayColorBrush" Color="{StaticResource ForegroundGrayColor}"/>
    
    <!-- Foreground   color -->
    <Color x:Key="ForegroundWhiteColor">White</Color>
    <SolidColorBrush x:Key="ForegroundWhiteColorBrush" Color="{StaticResource ForegroundWhiteColor}"/>
    
    <!-- Background color -->
    <Color x:Key="BackgroundWhiteColor">White</Color>
    <SolidColorBrush x:Key="BackgroundWhiteColorBrush" Color="{StaticResource BackgroundWhiteColor}"/>
    
    
    
</ResourceDictionary>